<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient android:type="linear" android:startColor="#629cfc" android:endColor="#6283fd" android:angle="315" />
        </shape>
    </item>
    <item
        android:left="12dp"
        android:top="12dp"
        android:right="12dp"
        android:bottom="12dp"
        >
        <bitmap android:src="@drawable/menu_icon"
            android:mipMap="true"
            />
    </item>
</layer-list>